{
	"name": "cfml-open-path",
	"publisher": "Scheifele",
	"displayName": "CFML Open Path",
	"description": "Open template path of the current cursor position",
	"repository": "https://github.com/XylianZeref/cfml-open-path",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cfml-open-path.openpath"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cfml-open-path.openpath",
				"title": "Open .cfm template"
			}
		],
        "keybindings": [
            {
                "command": "cfml-open-path.openpath",
                "key": "alt+d",
                "mac": "shift+cmd+d"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
